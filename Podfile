
project 'ThoughtWorkWebChat'

platform :ios, '9.0'
use_frameworks!
inhibit_all_warnings!

target 'ThoughtWorkWebChat' do
  # 网络请求
  pod 'AFNetworking'                      , '3.2.1'
  # YYKit 的一套分类
  pod 'YYCategories'                      , '1.0.4'
  # 富文本
  pod 'YYText'                            , '1.0.7'
  # 字典转模型
  pod 'YYModel'
  # YYImage
  pod 'YYImage'                           , '1.0.4'
  # 高性能的 iOS 异步图像加载框架。
  pod 'YYWebImage'                        , '1.0.5'
  # 高性能的 iOS 缓存框架
  pod 'YYCache'                           , '1.0.4'
  # 导航栏统一管理方案
  pod 'YPNavigationBarTransition', '~>2.0.9'
  pod 'Masonry', '1.1.0'
  pod 'MJRefresh', '3.1.15.7'
  # 侧滑手势
  pod 'FDFullscreenPopGesture'
  pod 'ReactiveObjC'
  # FaceBook 内存分析
  pod 'MLeaksFinder', '~> 1.0.0'
  #FPS检测
  pod 'JPFPSStatus', '0.1'
end

target 'ThoughtWorkWebChatTests' do
end

target 'ThoughtWorkWebChatUITests' do
end
